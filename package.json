{
  "name": "grafana-guidedtour-panel",
  "version": "0.3.1",
  "description": "Guided tour for Grafana dashboards",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "watch": "yarn dev"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "12.3.1",
    "@grafana/i18n": "12.3.1",
    "@grafana/runtime": "12.3.1",
    "@grafana/schema": "12.3.1",
    "@grafana/ui": "12.3.1",
    "emotion": "10.0.27",
    "lodash": "^4.17.21",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-joyride": "2.9.3",
    "semver": "^7.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.1.0",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin-ts": "^4.0.0",
    "@swc/core": "^1.14.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.23",
    "@types/markdown-it": "^14.0.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^6.22.0",
    "css-loader": "^6.7.3",
    "csstype": "^3.2.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^51.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.2",
    "sass-loader": "13.3.3",
    "semver": "^7.6.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.1",
    "typescript": "5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "packageManager": "yarn@4.12.0"
}
