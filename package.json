{
  "name": "grafana-guidedtour-panel",
  "version": "0.2.0",
  "description": "Guided tour for Grafana dashboards",
  "scripts": {
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "test": "grafana-toolkit plugin:test",
    "build": "grafana-toolkit plugin:build",
    "ci-build": "grafana-toolkit plugin:ci-build"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@grafana/data": "8.1.0",
    "@grafana/runtime": "8.1.0",
    "@grafana/ui": "8.1.0",
    "emotion": "10.0.27",
    "lodash": "^4.17.21",
    "react-joyride": "^2.3.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@grafana/e2e": "8.1.0",
    "@grafana/e2e-selectors": "8.1.0",
    "@grafana/toolkit": "8.1.0",
    "@types/lodash": "^4.14.170",
    "@types/markdown-it": "^12.0.2",
    "cspell": "^6.22.0",
    "csstype": "^3.0.9",
    "tslib": "^2.3.0"
  },
  "resolutions": {
    "rxjs": "6.3.3"
  }
}
